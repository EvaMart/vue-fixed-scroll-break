{
  "name": "vue-fixed-scroll-break",
  "version": "1.4.3",
  "description": "A performant wrapper component to make an element fixed and stop it from scrolling past a certain point.",
  "author": "evamart",
  "license" : "MIT",
  "keywords": [
    "vue",
    "nuxt",
    "vuetify",
    "fixed",
    "component",
    "scroll",
    "vue-fixed-scroll-break"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "dist/VueFixedScrollBreak.umd.js",
  "module": "dist/VueFixedScrollBreak.esm.js",
  "unpkg": "dist/VueFixedScrollBreak.min.js",
  "browser": {
    "./sfc": "src/VueFixedScrollBreak.vue"
  },
  "files": [
    "dist/*"
  ],
  "vetur": {
    "tags": "tags.json",
    "attributes": "attributes.json"
  },
  "scripts": {
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/VueFixedScrollBreak.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/VueFixedScrollBreak.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/VueFixedScrollBreak.min.js"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "minimist": "^1.2.0",
    "rollup": "^1.14.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.7.2",
    "vue-template-compiler": "^2.6.10"
  }
}